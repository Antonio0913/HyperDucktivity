{
    "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
    ],
    "env": {
        "node": true,
        "es2021": true,
        "jest": true
    },
    "parserOptions": {
    "ecmaFeatures": {
    "jsx": true
    },
    "sourceType": "module"
    },
    "rules": {
        "react/prop-types": 0,
        "react/react-in-jsx-scope": "off"
    },
    "settings": {
    "react": {
    "version": "detect"
    }
    }
}
    